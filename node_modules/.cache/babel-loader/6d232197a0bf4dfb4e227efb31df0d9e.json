wBAAA;AACA;AACA,KAVA;AAWA;AACA,IAAA,OAZA,qBAYA;AACA;AACA,UAAA,UAAA,GAAA,uBAAA;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,cAAA,CAAA,SAAA,EAAA,kBAAA;;AACA,UAAA,UAAA,CAAA,IAAA,CAAA,KAAA,OAAA,CAAA,EAAA;AACA,QAAA,UAAA,CAAA,SAAA,GAAA,8BAAA;AACA,OAFA,MAEA;AACA,QAAA,UAAA,CAAA,SAAA,GAAA,wBAAA;AACA;AACA,KArBA;AAsBA,IAAA,MAtBA,oBAsBA;AACA,MAAA,MAAA,CAAA,SAAA,GAAA,MAAA;AACA,MAAA,EAAA,CAAA,SAAA,GAAA,EAAA;AACA,KAzBA;AA0BA;AACA,IAAA,UAAA,EAAA,QAAA,CAAA,YAAA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,UAAA,QAAA,GAAA;AACA,QAAA,QAAA,EAAA,KAAA,QADA;AAEA,QAAA,OAAA,EAAA,KAAA,OAFA;AAGA,QAAA,oBAAA,EAAA,KAAA;AAHA,OAAA;AAKA,WAAA,KAAA,CACA,IADA,YACA,QADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,KAAA,CAAA,SAAA,EAAA,GAAA,CAAA,IAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,OAAA;AACA,OALA,WAMA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OARA;AASA,KAhBA,EAgBA,IAhBA,CA3BA;AA4CA;AACA,IAAA,YAAA,EAAA,QAAA,CAAA,YAAA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,aAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KAJA,EAIA,IAJA;AA7CA;AAVA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 遮罩层 -->\r\n    <div id=\"bg\"></div>\r\n    <div id=\"signIn\">\r\n      <div class=\"signInImg\"></div>\r\n      <div class=\"form\">\r\n        <div>\r\n          <h3>Welcome</h3>\r\n          <label for=\"userName\">\r\n            用&emsp;户&emsp;名:<input\r\n              @blur=\"uNameTest\"\r\n              class=\"mlm\"\r\n              type=\"text\"\r\n              placeholder=\"数字字母下划线组成\"\r\n              id=\"userName\"\r\n              v-model=\"username\"\r\n            />\r\n            <span></span> </label\r\n          ><br />\r\n          <label for=\"userPwd\">\r\n            密&emsp;码:<input\r\n              @blur=\"pwdTest\"\r\n              class=\"mln\"\r\n              type=\"password\"\r\n              placeholder=\"请输入密码\"\r\n              id=\"userPwd\"\r\n              v-model=\"userpwd\"\r\n            />\r\n            <span></span> </label\r\n          ><br />\r\n          <label for=\"userVerificationCode\">\r\n            验&emsp;证&emsp;码:\r\n            <input\r\n              type=\"text\"\r\n              id=\"userVerificationCode\"\r\n              v-model=\"userVerificationCode\"\r\n            />\r\n            <div id=\"captcha\" @click=\"clickCaptcha\" v-html=\"captchaData\"></div>\r\n          </label>\r\n          <div class=\"btn\">\r\n            <button @click=\"postSignIn\">登录</button>\r\n            <button @click=\"cancel\" >取消</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { debounce, thorttle } from \"../util/index.js\";\r\n\r\nexport default {\r\n  name: \"signIn\",\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      userpwd: \"\",\r\n      userVerificationCode: \"\",\r\n      captchaData: \"获取验证码\",\r\n    };\r\n  },\r\n  methods: {\r\n    // 用户名格式检测正则\r\n    uNameTest() {\r\n      var input_span = document.getElementById(\"userName\").nextElementSibling;\r\n      var reg = /^[a-zA-Z0-9_]{4,16}$/i; //4到16位（字母，数字，下划线，减号）\r\n      if (reg.test(this.username)) {\r\n        input_span.className = \" iconfont icon-gouxuan green\";\r\n      } else {\r\n        input_span.className = \" iconfont icon-cha red\";\r\n      }\r\n    },\r\n    //密码格式正则\r\n    pwdTest() {\r\n      //密码正则，最少6位，以字母开头，以五个数字字母下划线结尾\r\n      var pwdPattern = /^[a-zA-Z]{1}\\w{5,17}$/;\r\n      var input_span = document.getElementById(\"userPwd\").nextElementSibling;\r\n      if (pwdPattern.test(this.userpwd)) {\r\n        input_span.className = \" iconfont icon-gouxuan green\";\r\n      } else {\r\n        input_span.className = \" iconfont icon-cha red\";\r\n      }\r\n    },\r\n    cancel() {\r\n      signIn.className = \"none\";\r\n      bg.className = \"\";\r\n    },\r\n    // 防抖\r\n    postSignIn: debounce(function () {\r\n      let that = this;\r\n      let signInfo = {\r\n        username: this.username,\r\n        userpwd: this.userpwd,\r\n        userVerificationCode: this.userVerificationCode,\r\n      };\r\n      this.$http\r\n        .post(`/signin`, signInfo)\r\n        .then(function (res) {\r\n          that.$emit('resdata',res.data)\r\n          that.$parent.hideBtn();\r\n        })\r\n        .catch(function (err) {\r\n          console.log(err);\r\n        });\r\n    }, 1000),\r\n    // 节流\r\n    clickCaptcha: thorttle(function () {\r\n      this.$http.get(`/captcha`).then((res) => {\r\n        this.captchaData = res.data.data;\r\n      });\r\n    }, 1000),\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.bg {\r\n  height: 100%;\r\n  width: 100%;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  z-index: 100;\r\n}\r\n\r\n#signIn {\r\n  display: none;\r\n  height: 350px;\r\n  width: 840px;\r\n  background-color: bisque;\r\n  position: fixed;\r\n  z-index: 999;\r\n  top: 200px;\r\n  left: 0px;\r\n  right: 0px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n\r\n\r\n  .signInImg {\r\n    height: 100%;\r\n    width: 500px;\r\n    background: url(../static/imgs/signIn.jpeg) no-repeat;\r\n    background-size: 100% 100%;\r\n    float: left;\r\n  }\r\n}\r\n\r\n.form {\r\n  height: 100%;\r\n  display: flex;\r\n  align-content: center;\r\n  justify-content: center;\r\n\r\n  input {\r\n    height: 30px;\r\n    width: 120px;\r\n    margin: 10px;\r\n    border-radius: 10px;\r\n    border-color: #eaffea;\r\n    padding-left: 10px;\r\n  }\r\n\r\n  div {\r\n    h3 {\r\n      text-align: center;\r\n      margin: 30px 0;\r\n    }\r\n\r\n    label {\r\n      #userVerificationCode + div {\r\n        height: 30px;\r\n        width: 100px;\r\n        background-color: rgb(33, 128, 211);\r\n        display: inline-block;\r\n        text-align: center;\r\n        line-height: 30px;\r\n        border-radius: 30px;\r\n        cursor: pointer;\r\n        vertical-align: middle;\r\n      }\r\n    }\r\n\r\n    .btn {\r\n      margin: 20px 0;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: space-around;\r\n\r\n      button {\r\n        height: 30px;\r\n        width: 120px;\r\n        border-radius: 30px;\r\n        border: none;\r\n        cursor: pointer;\r\n      }\r\n\r\n      button:hover {\r\n        background-color: sandybrown;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.mln {\r\n  margin-left: 36px !important;\r\n}\r\n\r\n.mlm {\r\n  margin-left: 15px !important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}